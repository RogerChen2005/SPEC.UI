
{
        "code": "import React, { useState } from 'react';\nimport { Layout, Tag, Tabs, Avatar, Input, Button, Switch, Card, Row, Col } from 'antd';\nimport { \n  BarChart, Bar, XAxis, YAxis, CartesianGrid, Tooltip, Legend, \n  ScatterChart, Scatter, \n  LineChart, Line, \n  ResponsiveContainer, \n  RadialBarChart, RadialBar,\n  HeatMap\n} from 'recharts';\n\nconst { Header, Content } = Layout;\nconst { Search } = Input;\nconst { TabPane } = Tabs;\n\n// Custom SVG Icons\nconst AppLogoIcon = () => (\n  <svg width=\"24\" height=\"24\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n    <rect x=\"3\" y=\"3\" width=\"18\" height=\"18\" rx=\"2\" stroke=\"#1890FF\" strokeWidth=\"2\"/>\n    <circle cx=\"12\" cy=\"12\" r=\"4\" fill=\"#1890FF\"/>\n  </svg>\n);\n\nconst UserIcon = () => (\n  <svg width=\"20\" height=\"20\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n    <circle cx=\"12\" cy=\"8\" r=\"4\" stroke=\"#333333\" strokeWidth=\"2\"/>\n    <path d=\"M5 20C5 16.134 8.13401 13 12 13C15.866 13 19 16.134 19 20\" stroke=\"#333333\" strokeWidth=\"2\"/>\n  </svg>\n);\n\nconst PublishIcon = () => (\n  <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n    <path d=\"M12 3L3 12H7V20H17V12H21L12 3Z\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\"/>\n  </svg>\n);\n\nconst ResetIcon = () => (\n  <svg width=\"16\" height=\"16\" viewBox=\"0 0 24 24\" fill=\"none\" xmlns=\"http://www.w3.org/2000/svg\">\n    <path d=\"M2 4V10H8\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\"/>\n    <path d=\"M22 20V14H16\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\" strokeLinejoin=\"round\"/>\n    <path d=\"M20 11C19.1111 8.76785 17.2082 6.86494 14.9761 5.97609C12.744 5.08724 10.2994 5.25907 8.02388 6.46447C5.74835 7.66987 3.94226 9.90884 3.02388 12.6645C2.1055 15.4201 2.1055 18.5799 3.02388 21.3355C3.94226 24.0912 5.74835 26.3301 8.02388 27.5355C10.2994 28.7409 12.744 28.9128 14.9761 28.0239C17.2082 27.1351 19.1111 25.2321 20 23\" stroke=\"currentColor\" strokeWidth=\"2\" strokeLinecap=\"round\"/>\n  </svg>\n);\n\n// Mock data\nconst kpiData = [\n  { month: '1\u6708', value1: 40, value2: 60, value3: 20 },\n  { month: '2\u6708', value1: 55, value2: 45, value3: 35 },\n  { month: '3\u6708', value1: 30, value2: 70, value3: 50 },\n  { month: '4\u6708', value1: 65, value2: 35, value3: 45 },\n];\n\nconst scatterData = [\n  { x: 100, y: 200, z: 200 },\n  { x: 120, y: 100, z: 260 },\n  { x: 170, y: 300, z: 400 },\n  { x: 140, y: 250, z: 280 },\n  { x: 150, y: 400, z: 500 },\n  { x: 110, y: 280, z: 200 },\n];\n\nconst barData = [\n  { name: '\u4ea7\u54c1A', value: 4000 },\n  { name: '\u4ea7\u54c1B', value: 3000 },\n  { name: '\u4ea7\u54c1C', value: 2000 },\n  { name: '\u4ea7\u54c1D', value: 2780 },\n  { name: '\u4ea7\u54c1E', value: 1890 },\n];\n\nconst stepLineData = [\n  { name: '1\u6708', value: 1200 },\n  { name: '2\u6708', value: 1900 },\n  { name: '3\u6708', value: 1500 },\n  { name: '4\u6708', value: 2200 },\n  { name: '5\u6708', value: 1800 },\n  { name: '6\u6708', value: 2500 },\n];\n\nconst lineData = [\n  { name: '1\u6708', value: 400 },\n  { name: '2\u6708', value: 900 },\n  { name: '3\u6708', value: 700 },\n  { name: '4\u6708', value: 1200 },\n  { name: '5\u6708', value: 800 },\n  { name: '6\u6708', value: 1500 },\n];\n\nconst gaugeData = [\n  { name: '\u5b8c\u6210\u5ea6', value: 75 }\n];\n\nconst heatmapData = [\n  { x: '\u5468\u4e00', y: '00:00', value: 12 },\n  { x: '\u5468\u4e8c', y: '00:00', value: 23 },\n  { x: '\u5468\u4e09', y: '00:00', value: 34 },\n  { x: '\u5468\u56db', y: '00:00', value: 45 },\n  { x: '\u5468\u4e94', y: '00:00', value: 56 },\n  { x: '\u5468\u516d', y: '00:00', value: 67 },\n  { x: '\u5468\u65e5', y: '00:00', value: 78 },\n  { x: '\u5468\u4e00', y: '06:00', value: 23 },\n  { x: '\u5468\u4e8c', y: '06:00', value: 34 },\n  { x: '\u5468\u4e09', y: '06:00', value: 45 },\n  { x: '\u5468\u56db', y: '06:00', value: 56 },\n  { x: '\u5468\u4e94', y: '06:00', value: 67 },\n  { x: '\u5468\u516d', y: '06:00', value: 78 },\n  { x: '\u5468\u65e5', y: '06:00', value: 89 },\n];\n\nconst legendItems = [\n  { name: '\u6536\u5165', color: '#40A9FF' },\n  { name: '\u7528\u6237\u589e\u957f', color: '#52C41A' },\n  { name: '\u8f6c\u5316\u7387', color: '#FAAD14' },\n  { name: '\u5229\u6da6', color: '#FF4D4F' },\n  { name: '\u6210\u672c', color: '#9254DE' },\n];\n\nconst App = () => {\n  const [activeTab, setActiveTab] = useState('1');\n  const [timeSwitch, setTimeSwitch] = useState(false);\n\n  const renderKpiBar = (data) => (\n    <div style={{ display: 'flex', height: 20, borderRadius: 4, overflow: 'hidden' }}>\n      <div style={{ width: `${data.value1}%`, backgroundColor: '#40A9FF' }}></div>\n      <div style={{ width: `${data.value2}%`, backgroundColor: '#9254DE' }}></div>\n      <div style={{ width: `${data.value3}%`, backgroundColor: '#FF4D4F' }}></div>\n    </div>\n  );\n\n  return (\n    <Layout style={{ minHeight: '100vh', background: '#F0F2F5' }}>\n      {/* Global Header Section */}\n      <Header \n        style={{ \n          background: '#FFFFFF', \n          borderBottom: '1px solid #D9D9D9',\n          padding: '0 24px',\n          height: '64px',\n          display: 'flex',\n          alignItems: 'center',\n          justifyContent: 'space-between'\n        }}\n        data-spec=\"global-header-section\"\n      >\n        <div style={{ display: 'flex', alignItems: 'center' }}>\n          <Tag \n            style={{ \n              fontSize: '20px', \n              fontWeight: 'bold', \n              color: '#333333',\n              background: 'transparent',\n              border: 'none',\n              padding: 0,\n              height: 'auto'\n            }}\n            data-spec=\"app-title\"\n          >\n            <AppLogoIcon style={{ marginRight: 8 }} />\n            \u6570\u636e\u5206\u6790\u4eea\u8868\u677f\n          </Tag>\n        </div>\n        \n        <Tabs \n          defaultActiveKey=\"1\" \n          activeKey={activeTab}\n          onChange={setActiveTab}\n          tabBarStyle={{ \n            border: 'none', \n            margin: 0 \n          }}\n          data-spec=\"main-navigation-tabs\"\n        >\n          <TabPane tab=\"\u6982\u89c8\" key=\"1\" />\n          <TabPane tab=\"\u6536\u5165\u5206\u6790\" key=\"2\" />\n          <TabPane tab=\"\u7528\u6237\u5206\u6790\" key=\"3\" />\n          <TabPane tab=\"\u4ea7\u54c1\u8868\u73b0\" key=\"4\" />\n          <TabPane tab=\"\u5e02\u573a\u8d8b\u52bf\" key=\"5\" />\n        </Tabs>\n        \n        <div style={{ display: 'flex', alignItems: 'center' }}>\n          <span style={{ color: '#333333', marginRight: 8 }}>\u7ba1\u7406\u5458</span>\n          <Avatar \n            size={32} \n            style={{ \n              backgroundColor: '#D9D9D9', \n              color: '#333333',\n              display: 'flex',\n              alignItems: 'center',\n              justifyContent: 'center'\n            }}\n            icon={<UserIcon />}\n            data-spec=\"user-avatar\"\n          />\n        </div>\n      </Header>\n\n      {/* Filter Toolbar Section */}\n      <Content \n        style={{ \n          padding: '24px 24px 0',\n          background: '#FFFFFF',\n          borderBottom: '1px solid #D9D9D9'\n        }}\n        data-spec=\"filter-toolbar-section\"\n      >\n        <div style={{ display: 'flex', alignItems: 'center', gap: 8 }}>\n          <Search \n            placeholder=\"\u9009\u62e9\u8fc7\u6ee4\u6761\u4ef6\" \n            style={{ width: 200 }}\n            data-spec=\"filter-select-c18\"\n          />\n          <Button \n            type=\"primary\" \n            style={{ background: '#1890FF' }}\n            data-spec=\"filter-submit-button\"\n          >\n            \u5e94\u7528\u8fc7\u6ee4\n          </Button>\n          <Button \n            style={{ border: '1px solid #D9D9D9', color: '#333333' }}\n            icon={<ResetIcon />}\n            data-spec=\"filter-reset-button\"\n          >\n            \u91cd\u7f6e\n          </Button>\n          <div style={{ display: 'flex', alignItems: 'center', marginLeft: 16 }}>\n            <span style={{ color: '#333333', marginRight: 8 }}>\u4e0a\u6708</span>\n            <Switch \n              checked={timeSwitch}\n              onChange={setTimeSwitch}\n              style={{ background: timeSwitch ? '#1890FF' : '#D9D9D9' }}\n              data-spec=\"filter-time-switch\"\n            />\n            <span style={{ color: '#333333', marginLeft: 8 }}>\u672c\u6708</span>\n          </div>\n          <Button \n            type=\"primary\" \n            style={{ \n              background: '#1890FF', \n              borderColor: '#1890FF',\n              marginLeft: 'auto'\n            }}\n            icon={<PublishIcon />}\n            data-spec=\"publish-report-button\"\n          >\n            \u53d1\u5e03\u62a5\u544a\n          </Button>\n        </div>\n      </Content>\n\n      {/* Dashboard Grid Section */}\n      <Content \n        style={{ \n          padding: 24,\n          background: '#F0F2F5'\n        }}\n        data-spec=\"dashboard-grid-section\"\n      >\n        <Row gutter={[24, 24]}>\n          {/* KPI Snapshot Card - Spans both columns */}\n          <Col span={24}>\n            <Card \n              title=\"\u5173\u952e\u6307\u6807\u5feb\u7167\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"kpi-snapshot-card\"\n            >\n              <Row gutter={[16, 16]}>\n                {kpiData.map((item, index) => (\n                  <Col span={6} key={index}>\n                    <div style={{ display: 'flex', justifyContent: 'space-between', alignItems: 'center' }}>\n                      <span style={{ color: '#333333' }}>{item.month}</span>\n                      {renderKpiBar(item)}\n                    </div>\n                  </Col>\n                ))}\n              </Row>\n            </Card>\n          </Col>\n\n          {/* Chart Legend Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u56fe\u8868\u56fe\u4f8b\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"chart-legend-card\"\n            >\n              <div style={{ display: 'flex', flexDirection: 'column', gap: 16 }}>\n                {legendItems.map((item, index) => (\n                  <div key={index} style={{ display: 'flex', alignItems: 'center' }}>\n                    <div style={{ width: 12, height: 12, backgroundColor: item.color, marginRight: 8 }}></div>\n                    <span style={{ color: '#595959' }}>{item.name}</span>\n                  </div>\n                ))}\n              </div>\n            </Card>\n          </Col>\n\n          {/* Scatter Plot Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u6563\u70b9\u56fe\u5206\u6790\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"scatter-plot-card\"\n            >\n              <ResponsiveContainer width=\"100%\" height={300}>\n                <ScatterChart>\n                  <CartesianGrid stroke=\"#E8E8E8\" />\n                  <XAxis type=\"number\" dataKey=\"x\" stroke=\"#333333\" />\n                  <YAxis type=\"number\" dataKey=\"y\" stroke=\"#333333\" />\n                  <Tooltip />\n                  <Scatter name=\"\u6570\u636e\u70b9\" data={scatterData} fill=\"#1890FF\" />\n                </ScatterChart>\n              </ResponsiveContainer>\n            </Card>\n          </Col>\n\n          {/* Horizontal Bar Chart Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u6a2a\u5411\u67f1\u72b6\u56fe\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"horizontal-bar-chart-card\"\n            >\n              <ResponsiveContainer width=\"100%\" height={300}>\n                <BarChart\n                  layout=\"vertical\"\n                  data={barData}\n                  margin={{ top: 5, right: 30, left: 20, bottom: 5 }}\n                >\n                  <CartesianGrid stroke=\"#E8E8E8\" />\n                  <XAxis type=\"number\" stroke=\"#333333\" />\n                  <YAxis dataKey=\"name\" type=\"category\" stroke=\"#333333\" />\n                  <Tooltip />\n                  <Legend />\n                  <Bar dataKey=\"value\" fill=\"#40A9FF\" />\n                </BarChart>\n              </ResponsiveContainer>\n            </Card>\n          </Col>\n\n          {/* Step Line Chart Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u9636\u68af\u6298\u7ebf\u56fe\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"step-line-chart-card\"\n            >\n              <ResponsiveContainer width=\"100%\" height={300}>\n                <LineChart data={stepLineData}>\n                  <CartesianGrid stroke=\"#E8E8E8\" />\n                  <XAxis dataKey=\"name\" stroke=\"#333333\" />\n                  <YAxis stroke=\"#333333\" />\n                  <Tooltip />\n                  <Line type=\"step\" dataKey=\"value\" stroke=\"#9254DE\" strokeWidth={2} dot={{ r: 4 }} />\n                </LineChart>\n              </ResponsiveContainer>\n            </Card>\n          </Col>\n\n          {/* Line Chart Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u8d8b\u52bf\u6298\u7ebf\u56fe\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"line-chart-card\"\n            >\n              <ResponsiveContainer width=\"100%\" height={300}>\n                <LineChart data={lineData}>\n                  <CartesianGrid stroke=\"#E8E8E8\" />\n                  <XAxis dataKey=\"name\" stroke=\"#333333\" />\n                  <YAxis stroke=\"#333333\" />\n                  <Tooltip />\n                  <Line type=\"monotone\" dataKey=\"value\" stroke=\"#FF4D4F\" strokeWidth={2} dot={{ r: 4 }} />\n                </LineChart>\n              </ResponsiveContainer>\n            </Card>\n          </Col>\n\n          {/* Performance Gauge Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u6027\u80fd\u4eea\u8868\u76d8\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"performance-gauge-card\"\n            >\n              <ResponsiveContainer width=\"100%\" height={300}>\n                <RadialBarChart \n                  innerRadius=\"20%\" \n                  outerRadius=\"90%\" \n                  data={gaugeData}\n                  startAngle={180} \n                  endAngle={0}\n                >\n                  <RadialBar\n                    minAngle={15}\n                    label={{ fill: '#333333', position: 'insideStart' }}\n                    background={{ fill: '#F0F2F5' }}\n                    dataKey=\"value\"\n                    fill=\"#52C41A\"\n                  />\n                </RadialBarChart>\n              </ResponsiveContainer>\n            </Card>\n          </Col>\n\n          {/* Heatmap Chart Card */}\n          <Col span={8}>\n            <Card \n              title=\"\u70ed\u529b\u56fe\u5206\u6790\" \n              style={{ background: '#FFFFFF' }}\n              data-spec=\"heatmap-chart-card\"\n            >\n              <div style={{ height: 300, display: 'flex', alignItems: 'center', justifyContent: 'center', color: '#595959' }}>\n                \u70ed\u529b\u56fe\u7ec4\u4ef6\u5360\u4f4d (HeatmapChart)\n              </div>\n            </Card>\n          </Col>\n        </Row>\n      </Content>\n    </Layout>\n  );\n};\n\nexport default App;"
}

